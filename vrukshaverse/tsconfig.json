{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(auth)/login.jsx",
    "app/onboarding.jsx",
    "app/onboarding.jsx",
    "nativewind-env.d.ts"
  ]
}